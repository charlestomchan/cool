{
    "name": "coolphp/framework",
    "description": "swoole协程框架",
    "type": "framework",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "charles",
            "email": "charlestomchan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "psr/container": "~1.0",
        "psr/log": "~1.0",
        "psr/simple-cache": "~1.0",
        "firebase/php-jwt": "^5.0",
        "ext-json":"*",
        "ext-redis":"*",
        "ext-pdo":"*",
        "ext-mbstring":"*",
        "ramsey/uuid": "^3.7"
